<Window x:Class="Hotel.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Hotel - Główne Okno" Height="400" Width="600">
    <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
        <Button x:Name="btnDodajGoscia" Content="Dodaj Gościa" Margin="5" Click="btnDodajGoscia_Click" />
        <Button x:Name="btnDodajRezerwacje" Content="Dodaj Rezerwację" Margin="5" Click="btnDodajRezerwacje_Click" />
        <Button x:Name="btnPodgladPokoi" Content="Podgląd Pokoi" Margin="5" Click="btnPodgladPokoi_Click" />
        <Button x:Name="btnPodgladGosci" Content="Podgląd Gości" Margin="5" Click="btnPodgladGosci_Click" />
        <Button x:Name="btnPodgladPracownikow" Content="Podgląd Pracowników" Margin="5" Click="btnPodgladPracownikow_Click" />
        <Button x:Name="btnListaPobytow" Content="Lista Pobytów" Margin="5" Click="btnListaPobytow_Click" />
    </StackPanel>
</Window>
